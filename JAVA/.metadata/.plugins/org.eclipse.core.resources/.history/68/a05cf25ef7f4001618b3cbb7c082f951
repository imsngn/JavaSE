package day04;

public class Player {
	private String name;
	private int Score;
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getScore() {
		return Score;
	}
	public void setScore(int score) {
		Score = score;
	}
	public Player(String name, int score) {
		super();
		this.name = name;
		Score = score;
	}
	
	
	
}
